---
import { GradientText } from './GradientText';

interface Props {
  classes?: string;
}

const { classes = "" } = Astro.props;
---

<GradientText>
  <h1 class="title">BORADUO</h1>
</GradientText>

<style is:global>
  .title  {
    font-family: "Press Start 2P", system-ui;
    font-weight: 400;
    font-style: normal;
    font-size: 3rem;
    padding-left:20px;
  }

</style>
